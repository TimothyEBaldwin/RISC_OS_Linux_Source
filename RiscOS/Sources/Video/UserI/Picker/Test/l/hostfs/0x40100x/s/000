R       RN      0

SL      RN      10
FP      RN      11
IP      RN      12
SP      RN      13
LR      RN      14
PC      RN      15

        EXPORT  xhostfs_host_vdu

        AREA    |SWI$$Code|, CODE, READONLY

xhostfs_host_vdu ROUT
        MOV     IP, LR
        SWI     &60100
        MOVVC   R, #0
        MOVS    PC, IP

        END
