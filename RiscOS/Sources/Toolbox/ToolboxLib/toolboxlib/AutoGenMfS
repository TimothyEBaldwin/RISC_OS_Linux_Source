Target		=	o.toolboxlib

ASM		=	objasm
CC		=	cc

ASMFlags	=	$(ASMExtra) -Stamp -NoCache
CFlags		=	$(CCExtra) -c -ffah -depend !Depend -I@,C:
LibFileFlags	=	$(LibFileExtra) -c

All:	$(Target)

$(Target):	$(ObjectsA-M) $(ObjectsN-S) $(ObjectsT-Z)
	Libfile $(LibFileFlags) -o $@ -v ViaFileA-M -v ViaFileN-S -v ViaFileT-Z

#VPATH = @

.SUFFIXES:	.o .c

.s.o:;		$(ASM) $(ASMFlags) -o $@ $<
.c.o:;		$(CC) $(CFlags) -o $@ $<

