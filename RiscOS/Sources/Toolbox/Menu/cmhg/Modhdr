#include "VersionNum"

initialisation-code:            Menu_init

finalisation-code:              Menu_finalise

service-call-handler:           Menu_services 0x44ec1, 0x44ec2, 0x44ec3

title-string:                   Menu

help-string:                    Menu Module_MajorVersion_CMHG Module_MinorVersion_CMHG

