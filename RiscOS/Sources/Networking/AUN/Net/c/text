struct eblk error_blocks[15] =
{
   {     0,         0 },
   { 0x309, "BadPort" },
   { 0x30c, "TooBig"  },
   { 0x316, "PtNtAlc" },
   { 0x317, "PortAlc" },
   { 0x318, "NoPorts" },
   { 0x31a, "NoStore" },
   { 0x111, "BadSWI"  },
   { 0x30d, "BadMask" },
   { 0x311, "NoHware" },
   { 0x319, "BadInt"  },
   { 0x306, "BadStn"  },
   { 0x307, "BadNet"  },
   {  0xde, "Channel" },
   {  0x00, "NotConf" },
};

