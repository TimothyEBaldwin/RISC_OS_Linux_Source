        AREA    |Asm$$Code|, CODE, READONLY

        ENTRY

r0      RN      0
r1      RN      1

        MOV     r0, #&4f
        ADR     r1, MyFile
        SWI     13              ; OS_Find
        MOV     r1, r0
loop
        SWI     10              ; OS_BGet
        SWICC   &40102
        BCC     loop
        MOV     r0, #0
        SWI     13              ; OS_Find
        SWI     17              ; OS_Exit

MyFile  DCB     "s.TML_HostFT",0

        END
