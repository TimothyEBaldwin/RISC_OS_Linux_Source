        AREA    |Asm$$Code|, CODE, READONLY

        ENTRY

r0      RN      0
r1      RN      1

Start   SWI     &40104
        CMP     r0, #-1
        SWINE   0
        MOV     r0, #126
        SWI     6               ; OS_Byte
        TEQ     r1, #&ff
        BNE     Start
        ADR     r0, EscapeError
        SWI     43              ; OS_GenerateError

EscapeError
        DCD     17
        DCB     "Escape",0

        END
