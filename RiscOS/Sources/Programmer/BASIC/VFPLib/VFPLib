  PROCvfp_addencoding("8"     ,"VSELEQ/GE/GT/VS A1"                      ,"111111100Vd[4]cc[10]Vn[3210]Vd[3210]101sz[0]Vn[4]0Vm[4]0Vm[3210]"             ,"")
  PROCvfp_addencoding("8"     ,"VMAXNM,VMINNM A2"                        ,"111111101Vd[4]00Vn[3210]Vd[3210]101sz[0]Vn[4]op[0]Vm[4]0Vm[3210]"             ,"")
  PROCvfp_addencoding("8"     ,"VCVTA/M/N/P (floating-point) A1"         ,"111111101Vd[4]1111rm[10]Vd[3210]101sz[0]op[0]1Vm[4]0Vm[3210]"                 ,"")
  PROCvfp_addencoding("8"     ,"VRINTA/M/N/P (floating-point) A1"        ,"111111101Vd[4]1110rm[10]Vd[3210]101sz[0]01Vm[4]0Vm[3210]"                     ,"")
  PROCvfp_addencoding("8"     ,"VRINTR/Z (floating-point) A1"            ,"cond[3210]11101Vd[4]110110Vd[3210]101sz[0]op[0]1Vm[4]0Vm[3210]"               ,"")
