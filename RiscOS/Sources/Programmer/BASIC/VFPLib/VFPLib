  REM ** SIMD **
  PROCvfp_addencoding("S$"  ,"VABA,VABAL A1"                           ,"1111001U[0]0Vd[4]size[10]Vn[3210]Vd[3210]0111Vn[4]Q[0]Vm[4]1Vm[3210]"         ,"")
