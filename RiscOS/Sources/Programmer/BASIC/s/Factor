;UNMIN1 BEQ UNMINS
; CMP R10,#""""
; BEQ QSTR
; CMP R10,#"+"
;UNPLUS LDREQB R10,[AELINE],#1 ;eq state maintained through jump table!
; B DOPLUS
;Factors must return valid condition codes!

FACTOR  LDRB    R10,[AELINE],#1
; CMP R10,#" "
; BEQ FACTOR
; CMP R10,#"-"
; BLE UNMIN1
DOPLUS  LDR     R4,[PC,R10,LSL #2]     ;get table offset
        ADD     PC,PC,R4               ;and go there
AJ4                             *       .+4
        &       FACERR-AJ4
        &       FACERR-AJ4
        &       FACERR-AJ4
        &       FACERR-AJ4
        &       FACERR-AJ4
        &       FACERR-AJ4
        &       FACERR-AJ4
        &       FACERR-AJ4
        &       FACERR-AJ4
        &       FACERR-AJ4
        &       FACERR-AJ4
