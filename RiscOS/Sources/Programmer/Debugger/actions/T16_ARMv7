B_T1(cond,imm8,nonstandard)
{
	COMMON
	params->width = 16;
	ONLY4(ARMv4T,ARMv5Tstar,ARMv6star,ARMv7);
	_UNPREDICTABLE(INITBLOCK);
	int32_t ofs = sextend(imm8<<1,9);
	OP_CC_ADDR(JUSTPARAMS,"B",cond,ofs,4);
	return;
}

B_T2(imm11,nonstandard)
