CMP_reg_T2(Rn,N,Rm,nonstandard)
{
	COMMONT16
	ONLY4(ARMv4T,ARMv5Tstar,ARMv6star,ARMv7);
	_UNPREDICTABLE(((!N)&&(Rm<8))||((Rn|(N<<3))==15)||(Rm==15))
	OP_S_CC_RN_RM(JUSTPARAMS,"CMP",false,cond,Rm,Rn|(N<<3));
	return;
}

