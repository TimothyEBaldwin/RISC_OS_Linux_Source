# Actions for ASIMD disassembly

VABA_VABAL_A1(U,D:Vd,size,N:Vn,Q,M:Vm,nonstandard)
{
	COMMON
	ONLY1(ASIMD);
	ASIMD_D_N_M(JUSTPARAMS,"VABA",size,U,Q,D_Vd,N_Vn,M_Vm);
	return;
}

VABA_VABAL_A2(U,D:Vd,size,N:Vn,M:Vm,nonstandard)
