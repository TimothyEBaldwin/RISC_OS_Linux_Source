VABS_A1(D:Vd,size,F,Q,M:Vm,nonstandard)
{
	COMMON
	if(F)
	{
		ONLY1(ASIMDFP);
		ASIMD_F32_D_M(JUSTPARAMS,"VABS",Q,D_Vd,M_Vm);
	}
	else
	{
		ONLY1(ASIMD);
		ASIMD_D_M(JUSTPARAMS,"VABS",size,0,Q,D_Vd,M_Vm);
	}
	return;
}

