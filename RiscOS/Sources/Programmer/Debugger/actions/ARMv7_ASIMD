{
	COMMON
	if(F)
	{
		ONLY1(ASIMDFP);
		ASIMD_F32_D_M(JUSTPARAMS,"VMUL",Q,D_Vd,N_Vn);
	}
	else
	{
		ONLY1(ASIMD);
		ASIMD_I_D_M(JUSTPARAMS,"VMUL",size,Q,D_Vd,N_Vn);
	}
	uint32_t index;
	if(size==1)
		index = (M<<1)|(Vm>>3);
	else
		index = M;
	scatf(params->buf,",D%d[%d]",Vm&~(size<<3),index);
	return;
}

