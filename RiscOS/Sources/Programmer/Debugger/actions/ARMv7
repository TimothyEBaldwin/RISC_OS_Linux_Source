BL_BLX_imm_A2(H,imm24,nonstandard)
{
	COMMON
	ONLY3(ARMv5Tstar,ARMv6Tstar,ARMv7);
	int32_t ofs = sextend((imm24<<2)|(H<<1),26);
	OP_CC_ADDR(JUSTPARAMS,"BLX",14,ofs,8);
	return;
}

