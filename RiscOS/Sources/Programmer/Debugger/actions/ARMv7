BKPT_A1(cond,imm12:imm4,nonstandard)
{
	COMMON
	ONLY3(ARMv5Tstar,ARMv6Tstar,ARMv7);
	_UNPREDICTABLE(cond!=14);
	sprintf(params->buf,"BKPT%s\t%s%s%04X",condition(JUSTPARAMS,cond),(params->opt->ual?"#":""),HEX,imm12_imm4);
	return;
}

