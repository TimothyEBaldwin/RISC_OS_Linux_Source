LSL_imm_A1(cond,S,Rd,imm5,Rm,nonstandard)
{
	COMMON
	if(params->opt->ual)
		OP_S_CC_RD_RM_IMM(JUSTPARAMS,"LSL",S,cond,Rd,Rm,DecodeImmShift(0,imm5).amt);
	else
		OP_S_CC_RD_RM_SHIFT(JUSTPARAMS,"MOV",S,cond,Rd,Rm,0,imm5);
	return;
}

