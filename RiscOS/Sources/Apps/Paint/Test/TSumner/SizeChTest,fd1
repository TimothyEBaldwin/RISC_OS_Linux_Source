size%=624788
DIM area% size%
OSCLI "Load trace "+STR$~(area%+4)
area%!0=size%
SYS "OS_SpriteOp",58 OR 256,area%,"screen",640,152
OSCLI "Save ModTrace "+STR$~(area%+4)+" +"+STR$~(area%!0-4)
*SetType ModTrace Sprite
END
