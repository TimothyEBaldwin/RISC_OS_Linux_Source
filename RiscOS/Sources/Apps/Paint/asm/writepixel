R0      RN      0
R1      RN      1
R2      RN      2
R3      RN      3
R4      RN      4
R5      RN      5
R6      RN      6
R7      RN      7
R8      RN      8
R9      RN      9

A1      RN      0
A2      RN      1
A3      RN      2
A4      RN      3
V1      RN      4
V2      RN      5
V3      RN      6
V4      RN      7
V5      RN      8
V6      RN      9

R       RN      0

SL      RN      10
FP      RN      11
IP      RN      12
SP      RN      13
LR      RN      14
PC      RN      15

	GET     Hdr:ListOpts
        GET     Hdr:APCS.<APCS>

        EXPORT  xosspriteop_write_pixel_colour
        EXPORT  xosspriteop_write_pixel_mask

        AREA    |SWI$$Code|, CODE, READONLY

xosspriteop_write_pixel_colour ROUT
        MOV     IP, SP
        STMFD   SP!, {V1, V2, V3, LR}
        LDMFD   IP, {R4, R5, R6}
        ORR     R0, R0, #&2A
        SWI     &2002E
        MOVVC   R, #0
	Return	"V1, V2, V3"

xosspriteop_write_pixel_mask ROUT
        MOV     IP, SP
        STMFD   SP!, {V1, V2, LR}
        LDMFD   IP, {R4, R5}
        ORR     R0, R0, #&2C
        SWI     &2002E
        MOVVC   R, #0
	Return	"V1, V2"

        END
