        AREA    |Stub$$Data|, DATA

|_k_data_start|
        GET     kernel/k_data.s
|_k_data_end|

|_clib_data_start|
        GET     clib/cl_data.s
        GET     clib/clibdata.s
|_clib_data_end|

        GBLS    Bodge1
        GBLS    Bodge2
|_rlib_data_start|
 [ :DEF:RISC_OSStubs
Bodge1 SETS " GET     rlib/rl_data.s"
Bodge2 SETS " GET     rlib/rlibdata.s"
 |
Bodge1 SETS ""
Bodge2 SETS ""
 ]
$Bodge1
$Bodge2
|_rlib_data_end|

        END
