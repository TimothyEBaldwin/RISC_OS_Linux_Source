CLXLIB    = C:CLX.o.clxlib

TBOXLIB   = C:tboxlibs.o.toolboxlib
FLEXLIB   = C:tboxlibs.o.flexlib
EVENTLIB  = C:tboxlibs.o.eventlib
WIMPLIB   = C:tboxlibs.o.wimplib
RENDERLIB = C:tboxlibs.o.renderlib
