 [ {FALSE}
;debug code
FSHOW   STMFD   SP!,{R14,R0,R10,R9}
        SWI     OS_WriteS
        =       "AA ",0
        MOV     R10,R0
        BL      WORDHX
        SWI     OS_WriteS
        =       " AX ",0
        MOV     R10,R1
        BL      WORDHX
        SWI     OS_WriteS
        =       " AG ",0
        MOV     R10,R2
        BL      WORDHX
        SWI     OS_WriteS
        =       " AS ",0
        MOV     R10,R3
        BL      WORDHX
        SWI     OS_NewLine
        SWI     OS_WriteS
        =       "WA ",0
        MOV     R10,R4
        BL      WORDHX
        SWI     OS_WriteS
        =       " WX ",0
        MOV     R10,R5
        BL      WORDHX
        SWI     OS_WriteS
        =       " WG ",0
        MOV     R10,R6
        BL      WORDHX
        SWI     OS_WriteS
        =       " WS ",0
        MOV     R10,R7
        BL      WORDHX
        SWI     OS_NewLine
        SWI     OS_WriteS
        =       "SP ",0
        MOV     R10,SP
        BL      WORDHX
        SWI     OS_WriteS
        =       " AELINE ",0
        MOV     R10,R11
        BL      WORDHX
        SWI     OS_WriteS
        =       " LINE ",0
        MOV     R10,R12
        BL      WORDHX
        SWI     OS_NewLine
        LDMFD   SP!,{R0,R10,R9,PC}
WORDSP  SWI     OS_WriteI+" "
 ]
;print R10
WORDHX  STMFD   SP!,{R0,R9}
        MOV     R9,#32-4
WORDLP  MOV     R0,R10,LSR R9
        AND     R0,R0,#15
        CMP     R0,#9
        ORRLS   R0,R0,#"0"
        ADDHI   R0,R0,#"A"-10
        SWI     OS_WriteC
        SUBS    R9,R9,#4
        BPL     WORDLP
        LDMFD   SP!,{R0,R9}
        MOV     PC,R14
