DIM C% 256
SYS "IXSupport_FindRoutine", &e1a0f00e TO A%
P%=C%
[

.fastswi%
mov pc, lr
nop

.test%
mov r5, lr

mov lr, pc
mov pc, r0
dcd 1 ; OS_WriteS
dcb "Hello "
dcb 0
align

bl fastswi%
dcd 1 ; OS_WriteS
dcb "World"
dcb 10
dcb 0

mov pc, r5
]
SYS "IXSupport_PatchTable", fastswi%, test%
CALL test%
