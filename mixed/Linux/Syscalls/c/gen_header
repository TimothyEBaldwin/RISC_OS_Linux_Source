#include <stdio.h>
#include "syscalls.h"

#include <asm/hwcap.h>
#include <asm/sigcontext.h>
#include <linux/errno.h>
#include <linux/fs.h>
#include <linux/limits.h>
#include <linux/mman.h>
#include <linux/sched.h>
#include <linux/signal.h>
#include <linux/stat.h>
#include <asm-generic/ucontext.h>

#define DEF2(name, value) printf("%s * 0x%x\nix_%s * 0x%x\n", name, value, name, value);
#define DEF(x) DEF2(#x, x)

int main(void) {

  DEF(PROT_NONE)
  DEF(PROT_READ)
  DEF(PROT_WRITE)
  DEF(PROT_EXEC)

  DEF(MAP_SHARED)
  DEF(MAP_PRIVATE)
  DEF(MAP_FIXED)
  DEF(MAP_ANONYMOUS)
  DEF(MAP_NORESERVE)

  DEF(CLONE_VM)

  DEF(SIG_BLOCK)
  DEF(SIG_UNBLOCK)
  DEF(SIG_SETMASK)

  DEF(HWCAP_SWP)
  DEF(HWCAP_HALF)
  DEF(HWCAP_THUMB)
  DEF(HWCAP_26BIT)
  DEF(HWCAP_FAST_MULT)
  DEF(HWCAP_FPA)
  DEF(HWCAP_VFP)
  DEF(HWCAP_EDSP)
  DEF(HWCAP_JAVA)
  DEF(HWCAP_IWMMXT)
  DEF(HWCAP_CRUNCH)
  DEF(HWCAP_THUMBEE)
  DEF(HWCAP_NEON)
  DEF(HWCAP_VFPv3)
  DEF(HWCAP_VFPv3D16)
  DEF(HWCAP_TLS)
  DEF(HWCAP_VFPv4)
  DEF(HWCAP_IDIVA)
  DEF(HWCAP_IDIVT)
  DEF(HWCAP_VFPD32)
  DEF(HWCAP_IDIV)
  DEF(HWCAP_LPAE)
  DEF(HWCAP_EVTSTRM)

  DEF(HWCAP2_AES)
  DEF(HWCAP2_PMULL)
  DEF(HWCAP2_SHA1)
  DEF(HWCAP2_SHA2)
  DEF(HWCAP2_CRC32)

  DEF2("struct_ucontext_mcontext", offsetof(struct ucontext, uc_mcontext))
  DEF2("struct_ucontext_registers", offsetof(struct ucontext, uc_mcontext.arm_r0))

#include "syscall_list.h"

  puts("\n END");

  return 0;
}
