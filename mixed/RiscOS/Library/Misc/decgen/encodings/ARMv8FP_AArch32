[AS1(U)]0D(size:2)(Vn:4)(Vd:4)0111NQM0(Vm:4)	{eq(size,3)}	UNDEFINED
[AS1(U)]0D(size:2)(Vn:4)(Vd:4)0111NQM0(Vm:4)	{ne(size,3)} {band(Q,bor(bor(Vd,Vn),Vm))}	UNDEFINED in VABD_VABDL_A1
