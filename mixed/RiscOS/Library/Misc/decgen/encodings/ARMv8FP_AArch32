[VFP1]11100(opc1:2)0(Vd:4)(Rt:4)1011D(opc2:2)1(0000)	[VFP2] {lnot(band(opc1,2))} {eq(opc2,2)}	UNDEFINED_MCR_MCR2_A1 in VMOV_arm_A1

