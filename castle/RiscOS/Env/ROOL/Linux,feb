set Locale     UK
set Keyboard   All
| Hardware target
set Machine    Linux
| Software target
set System     Ursula
set UserIF     Sovereign
set Display    PAL
set ImageSize  4096K
set HALSize    64K
set Build      ROOL.Linux
set APCS       APCS-32
Unset LocaleList
